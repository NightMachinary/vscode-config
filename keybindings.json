// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "alt+x",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.navigateRight"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.navigateDown"
    },
    { "key": "cmd+;", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus" },
    {
        "key": "cmd+;",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "editorTextFocus"
    },
    {
        "key": "backspace",
        "command": "language-julia.plotpane-delete",
        "when": "jlplotpaneFocus"
    },
    {
        "key": "shift+backspace",
        "command": "language-julia.plotpane-delete-all",
        "when": "jlplotpaneFocus"
    },
    { "key": "q", "command": "workbench.action.closeActiveEditor", "when": "jlplotpaneFocus" },
]